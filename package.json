{
    "name": "scss-generator",
    "displayName": "scss generator",
    "description": "generate scss markup from your html",
    "version": "1.0.0",
    "icon": "images/scsslogo.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "repository": "https://github.com/natqe/scss-generator",
    "categories": [
        "Other"
    ],
    "activationEvents": [
      "onLanguage:scss"
    ],
    "main": "./out/extension",
    "publisher": "natqe",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/jsdom": "^12.2.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "jsdom": "^13.1.0",
        "lodash": "^4.17.11",
        "utilizes": "^1.2.90"
    }
}