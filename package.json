{
    "name": "scss-generator",
    "displayName": "scss generator",
    "description": "generate scss markup from your html",
    "version": "1.2.8",
    "icon": "images/scsslogo.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "repository": "https://github.com/natqe/scss-generator",
    "categories": [
        "Snippets",
        "Formatters",
        "Programming Languages",
        "Linters",
        "Keymaps",
        "Other"
    ],
    "keywords": [
        "html to scss",
        "css",
        "scss",
        "sass",
        "html",
        "easy scss",
        "easy sass",
        "compile",
        "html to css",
        "html2css",
        "html2scss",
        "style",
        "compiler"
    ],
    "activationEvents": [
        "onLanguage:scss"
    ],
    "main": "./out/extension",
    "publisher": "natqe",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/jsdom": "^12.2.1",
        "@types/lodash": "^4.14.119",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "jsdom": "^13.1.0",
        "lodash": "^4.17.11",
        "utilizes": "^1.2.90"
    }
}
